{
  "name": "<%= `@${templateConfig.name}/portal`%>",
  "version": "1.0.0",
  "description": "<%= `@${templateConfig.name} design system portal`%>",
  "files": [
  ],
  "scripts": {
    "copy": "yarn copy:mdx && yarn copy:components",
    "copy:mdx": "ts-node ./scripts/copyMdx.ts",
    "copy:components": "ts-node ./scripts/copyDist.ts",
    "start": "yarn copy && yarn docz:start",
    "docz:start": "docz dev",
    "docz:build": "docz build",
    "build:portal": "yarn build:prepare && yarn build:install && yarn docz:build",
    "build:prepare": "yarn copy --build",
    "build:install": "yarn install"
  },
  "devDependencies": {
    "docz": "^2.0.0-rc.35",
    "<%= `@${templateConfig.name}/components` %>": "^1.0.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "5.2.1",
    "glob-promise": "3.4.0",
    "fs-extra": "^8.1.0",
    "ts-node": "^8.3.0",
    "npm-run-all": "^4.1.5",
    "extra-watch-webpack-plugin": "1.0.3",
    "yargs": "13.3.0"
  },
  "dependencies": {
    "gatsby-theme-docz": "^2.0.0-rc.35"
  },
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "linters": {
      "./src/**/*.{js,ts,tsx,html,scss,md,mdx}": [
        "yarn format",
        "git add"
      ]
    }
  },
  "license": "MIT",
  "workspaces": [
  "../../../packages/**/*",
  "../../packages/**/*"
  ],
  "private": true
}
