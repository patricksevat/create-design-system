{
  "name": "<%= `@${templateConfig.name}/components`%>",
  "version": "1.0.0",
  "description": "<%= `@${templateConfig.name} design system components`%>",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs-json",
    "start": "stencil build --dev --watch --serve --docs-json docs/components.json",
    "test:e2e": "wdio",
    "test": "stencil test --spec",
    "test:unit.watch": "stencil test --spec --watchAll",
    "pre-commit": "run-p lint test && lint-staged",
    "format": "prettier --write --config prettier.config.js \"src/**/*.{js,ts,tsx,html,scss,mdx,json}\" ",
    "lint": "run-p lint:*",
    "lint:typescript": "tslint -c tslint.json -p tsconfig.json",
    "lint:editor": "cross-var eclint check $PWD/src/**/*.{ts,tsx}",
    "lint:style": "stylelint \"src/**/*.scss\""
  },
  "dependencies": {
    "@stencil/core": "^1.2.4",
    "@stencil/sass": "^1.0.1"
  },
  "devDependencies": {
    "@types/jasmine": "^3.4.0",
    "@types/jest": "24.0.18",
    "@wdio/cli": "^5.12.4",
    "@wdio/jasmine-framework": "^5.12.1",
    "@wdio/local-runner": "^5.10.1",
    "@wdio/selenium-standalone-service": "^5.0.0",
    "@wdio/spec-reporter": "^5.0.0",
    "axe-core": "^3.3.0",
    "jest": "24.8.0",
    "jest-cli": "24.8.0",
    "wdio-image-comparison-service": "^1.5.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "linters": {
      "./src/**/*.{js,ts,tsx,html,scss,md,mdx}": [
        "yarn format",
        "git add"
      ]
    }
  },
  "license": "MIT"
}
